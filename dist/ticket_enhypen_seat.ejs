    <style type="text/tailwindcss">
        @tailwind base;
      @tailwind components;
      @tailwind utilities;
      @layer utility {
        .tigetbg {
          background-image: linear-gradient(black, black),
            url("./figmaAssets/cloud-bg.png");
          background-repeat: no-repeat;
          background-size: cover;
          background-blend-mode: saturation;
        }
        .tigetbginv {
          background-image: linear-gradient(black, black),
            url("./figmaAssets/cloud-bg-inv.png");
          background-repeat: no-repeat;
          background-size: cover;
          background-blend-mode: saturation;
        }
        .tigetbgfull {
          background-image: linear-gradient(black, black),
            url("./figmaAssets/cloud-bg-full.png");
          background-repeat: no-repeat;
          background-size: cover;
          background-blend-mode: saturation;
        }
      }
    </style>
    <script>
        tailwind.config = {
            theme: {
                extend: {
                    fontFamily: {
                        dmsans: ["DM Sans", "sans-serif"],
                        roboto: ["Roboto", "sans-serif"],
                    },
                    colors: {
                        clifford: "#da373d",
                        tigetgold: "#c09e6b",
                        tigetgold2: "#c09e6b",
                    },
                },
            },
        };
    </script>
</head>

<body class="tigetbg">
    <div class="container flex flex-col">
